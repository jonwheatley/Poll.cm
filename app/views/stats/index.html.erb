        data.setValue(26, 0, '4');
        data.setValue(26, 1, <%= @days[3] %>);
        data.setValue(27, 0, '3');
        data.setValue(27, 1, <%= @days[2] %>);
        data.setValue(28, 0, '2');
        data.setValue(28, 1, <%= @days[1] %>);
        data.setValue(29, 0, '1');
				data.setValue(29, 1, <%= @days[0] %>);

        var chart = new google.visualization.LineChart(document.getElementById('chart_div'));
        chart.draw(data, {width: 600, height: 300, title: 'Daily Votes', backgroundColor: '#dcdcdc', legend: 'none' });
